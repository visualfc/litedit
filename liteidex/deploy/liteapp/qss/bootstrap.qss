/*QPushButton*/
QPushButton
{
	border:1px solid transparent;
	border-radius:4px;
	color: #333;
    background-color: #fff;
	border-color: #ccc;
	font-size: 14px;
	font-weight: 400;
	min-height:28px;
}
QPushButton:hover
{
	background-color:#e6e6e6;
	border-color:#adadad;
}
QPushButton:pressed,QPushButton:checked
{
	background-color:#d6d6d6;
	border-color:#8c8c8c;
}
QPushButton[type="primary"]
{
    color: #fff;
    background-color: #337ab7;
    border-color: #2e6da4;
}
