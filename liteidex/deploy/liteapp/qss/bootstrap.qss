}
QPushButton[type="success"]:pressed,QPushButton[type="success"]:checked
{
	background-color: #398439;
    border-color: #255625;
}
QPushButton[type="info"]
{
    color: #fff;
    background-color: #5bc0de;
    border-color: #46b8da;
}
QPushButton[type="info"]:hover
{
    background-color: #31b0d5;
