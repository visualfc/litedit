/*QRadioButton & QCheckBox*/
QRadioButton,QCheckBox
{
	spacing:6px;
}
QRadioButton::indicator
{
	width:16px;
	height:16px;
	image:url(Resources/images/radio.png);
}
QRadioButton::indicator:disabled
{
	width:16px;
	height:16px;
	image:url(Resources/images/radio-disabled.png);
}
QRadioButton::indicator:checked
{
	width:16px;
	height:16px;
	image:url(Resources/images/radio-active.png);
}
QRadioButton::indicator:checked:disabled
{
	width:16px;
	height:16px;
	image:url(Resources/images/radio-active-disabled.png);
}
QCheckBox::indicator,QGroupBox::indicator
{
	width:16px;
	height:16px;
	image:url(Resources/images/checkbox.png);
}
QCheckBox::indicator:disabled,QGroupBox::indicator:disabled
{
	width:16px;
	height:16px;
	image:url(Resources/images/checkbox-disabled.png);
}
QCheckBox::indicator:checked,QGroupBox::indicator:checked
{
	width:16px;
	height:16px;
	image:url(Resources/images/checkbox-active.png);
}
QCheckBox::indicator:checked:disabled,QGroupBox::indicator:checked:disabled
{
	width:16px;
	height:16px;
	image:url(Resources/images/checkbox-active-disabled.png);
}
