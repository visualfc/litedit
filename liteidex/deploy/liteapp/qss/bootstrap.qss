}
QPushButton[type="info"]:pressed,QPushButton[type="info"]:checked
{
	background-color: #269abc;
    border-color: #1b6d85;
}
QPushButton[type="warning"]
{
    color: #fff;
    background-color: #f0ad4e;
    border-color: #eea236;
}
QPushButton[type="warning"]:hover
{
