<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Solar Sooty">
  <!--
  Based on Solar Sooty
  Improvements are welcome
  Public Domain, Unknown, 2014
  -->
  <style name="Text" foreground="#F8F8F2" background="#111"/>
  <style name="Extra" foreground="#008B8B" background=""/>
  <style name="Selection" background="#2D5D2D"/>
  <style name="CurrentLine" background="#2D2D2D"/>
  <style name="IndentLine" foreground="#008B8B"/>
  <style name="VisualWhitespace" foreground="#f57900"/>
  <style name="Keyword" foreground="#EC7600" bold="false"/>
  <style name="DataType" foreground="#2aa2d4" /> 
  <style name="Decimal" foreground="#AE81FF"/>
  <style name="BaseN" foreground="#FFFFFF"/>
  <style name="Float" foreground="#FFFFFF"/>
  <style name="Char" foreground="#a5120b"/>
  <style name="String" foreground="#F3E87E"/>
  <style name="Comment" foreground="#999999"/>
  <style name="Alert" foreground="#ff0000"/>
  <style name="Error" foreground="#ff0000"/>
  <style name="Function" foreground="#A6E22E"/>
  <style name="RegionMarker" foreground="#AE81FF"/>
  <style name="Symbol" foreground="#FFFFFF"/> 
  <style name="BuiltinFunc" foreground="#AE81FF"/>
  <style name="Predeclared" foreground="#3085c6"/>
  <style name="FuncDecl" foreground="#A6E22E"/>
  <style name="Placeholder" foreground="#a5120b"/>
  <style name="ToDo" foreground="#006000" background="#e0e0e0" bold="true"/>
</style-scheme>
